{
  "name": "rh-browser",
  "version": "0.0.7",
  "description": "方法包",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install",
    "commitlint": "commitlint -e",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "browser": "webpack serve --config ./webpack.dev.js",
    "dev_test": "ts-node-dev --respawn --transpile-only ./src/test/test_rh.ts",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node ./dist/index.js",
    "prod": "npm run build && npm run start",
    "pub": "npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "typescript"
  ],
  "author": "ruihuag",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.15",
    "@types/react-custom-scrollbars": "^4.0.8",
    "@types/react-dom": "^17.0.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitlint": "^17.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.6.1",
    "node-notifier": "^8.0.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resolve-url-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}